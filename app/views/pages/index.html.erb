<div class="row">
  <div class="span12">
    <%= image_tag 'sns-banner.png', class: 'main-banner' %>
  </div>
</div>
<div class="row">
  <div class="span12">
    <div id="main-carousel" class="carousel slide">
      <div class="carousel-inner">
        <div class="active item"><%= image_tag 'sns-banner.png' %></div>
        <div class="item"><%= image_tag 'sns_web_logo.png' %></div>
      </div>
      <a class="carousel-control left" href="#main-carousel" data-slide="prev">&lsaquo;</a>
      <a class="carousel-control right" href="#main-carousel" data-slide="next">&rsaquo;</a>
    </div>
  </div>
</div>
