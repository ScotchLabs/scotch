<%= simple_form_for(@allocation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :reserver_id %>
    <%= f.input :group_id %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :return_date %>
    <%= f.input :reason %>
    <%= f.input :approved %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
