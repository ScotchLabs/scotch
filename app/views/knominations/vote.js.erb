<% if @knomination.kudo.nominations_open? %>
  <% if @vote.nil? %>
    $("#<%= @knomination.id %>_up").addClass("notvoted");
  <% elsif @vote.positive %>
    $("#<%= @knomination.id %>_up").removeClass("notvoted");
  <% else %>
    $("#<%= @knomination.id %>_up").addClass("notvoted");
  <% end %>
<% elsif @knomination.kudo.voting_open? %>
  $("#kaward_<%= @knomination.kaward.id %> > .line-item > .knomination > .checkmark").addClass("notvoted");
  $("#knomination_<%= @knomination.id %> > .checkmark").removeClass("notvoted");
<% end %>
