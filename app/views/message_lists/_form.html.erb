<%= simple_form_for(@message_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :group_id %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :distribution %>
    <%= f.input :entire_group %>
    <%= f.input :security %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
