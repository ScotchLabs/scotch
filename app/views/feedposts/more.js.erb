$("#feedposts").append("<% @feedposts.each do |post| %><%= escape_javascript(render 'feedposts/' + post.post_type, :post => post) %><% end %>");

<% if @feedposts_count > @feedposts.length %>
$("#morebutton").replaceWith("<%= escape_javascript(link_to 'See 10 More', more_feedposts_path(:id => @feedposts.last.id), :remote => true, :id => 'morebutton', :class => 'button') %>");
<% else %>
$("#morebutton").replaceWith("");
<% end %>
